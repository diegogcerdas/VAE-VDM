#!/bin/bash

#SBATCH --partition=gpu_titanrtx_shared_course
#SBATCH --gres=gpu:1
#SBATCH --job-name=eval
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=3
#SBATCH --time=20:00:00
#SBATCH --mem=32000M
#SBATCH --output=slurm_output_eval_%A.out

module purge
module load 2022
module load Anaconda3/2022.05

source activate vdm

export CUDA_HOME=$CONDA_PREFIX

cd ~/VAE-VDM/src

EXP=exp05

srun python -u eval.py --results-path results/$EXP